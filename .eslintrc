{
  "parser": "babel-eslint",
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-unresolved": [
      2
    ],
    "space-before-function-paren": "off",
    "semi": "off"
  },
  "globals": {
    "Phaser": true
  },
  "settings": {
    "import/core-modules": [
      "phaser"
    ]
  }
}